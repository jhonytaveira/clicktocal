<h1>Listing register_numbers</h1>

<table>
  <thead>
    <tr>
      <th>Ip</th>
      <th>Ddd</th>
      <th>Number</th>
      <th>Carried out</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @register_numbers.each do |register_number| %>
      <tr>
        <td><%= register_number.ip %></td>
        <td><%= register_number.ddd %></td>
        <td><%= register_number.number %></td>
        <td><%= register_number.carried_out %></td>
        <td><%= link_to 'Show', register_number %></td>
        <td><%= link_to 'Edit', edit_register_number_path(register_number) %></td>
        <td><%= link_to 'Destroy', register_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Register number', new_register_number_path %>
